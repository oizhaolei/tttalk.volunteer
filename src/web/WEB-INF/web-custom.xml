<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <!-- requect -->
    <servlet>
        <servlet-name>VolunteerRequest</servlet-name>
        <servlet-class>org.tttalk.openfire.plugin.VolunteerRequestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VolunteerRequest</servlet-name>
        <url-pattern>/request</url-pattern>
    </servlet-mapping>
    
    <!-- cancel -->
    <servlet>
        <servlet-name>VolunteerCancelServlet</servlet-name>
        <servlet-class>org.tttalk.openfire.plugin.VolunteerCancelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VolunteerCancelServlet</servlet-name>
        <url-pattern>/cancel</url-pattern>
    </servlet-mapping>

	<!-- Servlets -->
    <servlet>
        <servlet-name>ChangePasswordServlet</servlet-name>
        <servlet-class>org.tttalk.openfire.plugin.ChangePasswordServlet</servlet-class>
    </servlet>
    <!-- Servlet mappings -->
    <servlet-mapping>
        <servlet-name>ChangePasswordServlet</servlet-name>
        <url-pattern>/change_user_pwd</url-pattern>
    </servlet-mapping>
        
    <!-- Servlets -->
    <servlet>
        <servlet-name>CreateAccountServlet</servlet-name>
        <servlet-class>org.tttalk.openfire.plugin.CreateAccountServlet</servlet-class>
    </servlet>
    <!-- Servlet mappings -->
    <servlet-mapping>
        <servlet-name>CreateAccountServlet</servlet-name>
        <url-pattern>/create_account</url-pattern>
    </servlet-mapping>
    
    
</web-app>
